{
  "main": "index.html",
  "logo": "logo.png",
  "development": {
    "main": "http://localhost:5173"
  },
  "features": [
    {
      "code": "convert",
      "explain": "Cookie to JSON",
      "cmds": [
        "Cookie2JSON",
        {
          "type": "regex", 
          "label": "Cookie转JSON",
          "match": "/^[^=;\\s]+=[^;]+(?:;\\s*[^=;\\s]+=[^;]+)*(?:;\\s*)?$/"
        },
        {
          "type": "regex", 
          "label": "curl Cookie转JSON",
          "match": "/^(?=[\\s\\S]*curl)(?=[\\s\\S]*([Cc]ookie|-b))[\\s\\S]*$/"
        }
      ]
    }
  ]
}
